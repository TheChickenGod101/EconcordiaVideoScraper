{
  "manifest_version": 3,
  "name": "Video Extractor",
  "description": "Extracts video URLs (including H5P embeds) from the current page and its iframes.",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Extract video URLs"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "downloads"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
